{
  "name": "@sutro-dev/route-parser",
  "version": "6.2.0",
  "description": "A isomorphic, bullet-proof, ninja-ready route parsing, matching, and reversing library for Javascript in Node and the browser. ",
  "keywords": [
    "url",
    "matching",
    "routing",
    "route",
    "regex",
    "match"
  ],
  "homepage": "https://github.com/SutroOrg/route-parser",
  "maintainers": [
    {
      "name": "Dan Rumney",
      "email": "dan@withsutro.com"
    }
  ],
  "author": {
    "name": "Ryan Sorensen",
    "email": "rcsorensen@gmail.com",
    "url": "https://github.com/rcs"
  },
  "exports": {
    ".": {
      "import": "./dist/route.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SutroOrg/route-parser.git"
  },
  "type": "module",
  "bugs": {
    "email": "eng@withsutro.com"
  },
  "engines": {
    "node": ">= 0.9"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsc && pnpm copy:parser",
    "copy:parser": "cp src/route/compiled-grammar.js dist/route/compiled-grammar.js",
    "compile-parser": "scripts/compile_parser.js",
    "clean-compile": "rm ./src/route/compiled-grammar.js",
    "lint": "eslint ."
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/config-helpers": "^0.4.0",
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jison": "~0.4.18",
    "jison-lex": "~0.3.4",
    "jsdoc": "~3.4.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
